<div style="background-color: #877EB4" class="uk-margin uk-margin-auto uk-card uk-card-default uk-card-body uk-box-shadow-large">
    <h3 style="color: black" class="uk-card-title uk-text-center">            Submissions to Date</h3>
    <table style="color: black" class="uk-table">
        <thead>
        <th>Student Name</th>
        <th>Project Title</th>
        <th>Descriptive Title</th>
        <th>Project Type</th>
        <th>Student Personal Photo</th>
        <th>Project Image</th>
        <th>Summary (<100 words)</th>
        <th>Project Landing Page URL</th>
        <th>Demonstration Video URL</th>
        </thead>
        <tbody class="uk-text-left">
        {{#each submissions}}
            <tr>
                <td>{{name}}</td>
                <td>{{projectTitle}}</td>
                <td>{{descriptiveTitle}}</td>
                <td>{{projectType}}</td>
                <td>{{personalPhoto}}</td>
                <td>{{projectImage}}</td>
                <td>{{summary}}</td>
                <td>{{projectUrl}}</td>
                <td>{{videoUrl}}</td>
            </tr>
        {{/each}}
        </tbody>
    </table>
</div>